<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="questionList"
		class="com.kajal.spring.question2.Question">
		<property name="questionId" value="1"></property>
		<property name="question"
			value="What is the capital of India ? "></property>
		<property name="answerList">
			<list>
				<value>Mumbai</value>
				<value>Delhi</value>
				<value>Bangalore</value>
				<value>Chennai</value>
			</list>
		</property>
	</bean>

	<bean id="questionSet" class="com.kajal.spring.question2.Question">
		<property name="questionId" value="2"></property>
		<property name="question" value="What is the capital of India ? "></property>
		<property name="answerSet">
			<set>
				<value>Mumbai</value>
				<value>Delhi</value>
				<value>Bangalore</value>
				<value>Chennai</value>
			</set>
		</property>
	</bean>

	<bean id="questionMap" class="com.kajal.spring.question2.Question">
		<property name="questionId" value="3"></property>
		<property name="question" value="What is the capital of India ? "></property>
		<property name="answerMap">
		<map>
			<entry key="1">
				<value>Mumbai</value>
			</entry>

			<entry key="2">
				<value>Delhi</value>
			</entry>

			<entry key="3">
				<value>Bangalore</value>
			</entry>

			<entry key="4">
				<value>Chennai</value>
			</entry>
		</map>
		</property>
	</bean>




</beans>
